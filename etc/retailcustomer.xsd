						<xs:annotation>
							<xs:appinfo>Read</xs:appinfo>
							<xs:documentation/>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="2">
						<xs:annotation>
							<xs:appinfo>Update</xs:appinfo>
							<xs:documentation/>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="3">
						<xs:annotation>
							<xs:appinfo>Delete</xs:appinfo>
							<xs:documentation/>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
